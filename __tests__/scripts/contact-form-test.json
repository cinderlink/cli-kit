{
  "name": "Contact Form Test",
  "description": "Test the contact form example",
  "steps": [
    {
      "description": "Wait for form to load",
      "action": "waitForText",
      "text": "Simple Contact Form",
      "timeout": 5000
    },
    {
      "description": "Take initial screenshot",
      "action": "screenshot",
      "name": "01-initial.png"
    },
    {
      "description": "Enter name",
      "action": "keys",
      "keys": [
        { "key": "J", "delay": 50 },
        { "key": "o", "delay": 50 },
        { "key": "h", "delay": 50 },
        { "key": "n", "delay": 50 },
        { "key": " ", "delay": 50 },
        { "key": "D", "delay": 50 },
        { "key": "o", "delay": 50 },
        { "key": "e", "delay": 50 }
      ],
      "delay": 200
    },
    {
      "description": "Tab to email field",
      "action": "key",
      "key": "tab",
      "delay": 200
    },
    {
      "description": "Enter email",
      "action": "keys",
      "keys": [
        { "key": "j", "delay": 30 },
        { "key": "o", "delay": 30 },
        { "key": "h", "delay": 30 },
        { "key": "n", "delay": 30 },
        { "key": "@", "delay": 30 },
        { "key": "e", "delay": 30 },
        { "key": "x", "delay": 30 },
        { "key": "a", "delay": 30 },
        { "key": "m", "delay": 30 },
        { "key": "p", "delay": 30 },
        { "key": "l", "delay": 30 },
        { "key": "e", "delay": 30 },
        { "key": ".", "delay": 30 },
        { "key": "c", "delay": 30 },
        { "key": "o", "delay": 30 },
        { "key": "m", "delay": 30 }
      ],
      "delay": 200
    },
    {
      "description": "Take screenshot with data",
      "action": "screenshot",
      "name": "02-filled.png"
    },
    {
      "description": "Tab to submit button",
      "action": "key",
      "key": "tab",
      "delay": 200
    },
    {
      "description": "Submit form",
      "action": "key",
      "key": "enter",
      "delay": 500
    },
    {
      "description": "Wait for success message",
      "action": "waitForText",
      "text": "Form Submitted Successfully",
      "timeout": 2000
    },
    {
      "description": "Take success screenshot",
      "action": "screenshot",
      "name": "03-success.png"
    },
    {
      "description": "Verify form data",
      "action": "assert",
      "contains": "Name: John Doe"
    },
    {
      "description": "Reset form",
      "action": "key",
      "key": "r",
      "delay": 500
    },
    {
      "description": "Verify reset",
      "action": "assert",
      "notContains": "Form Submitted Successfully"
    },
    {
      "description": "Exit application",
      "action": "key",
      "key": "q"
    }
  ]
}