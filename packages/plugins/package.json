{
  "name": "@tuix/plugins",
  "version": "1.0.0-rc.2",
  "description": "Production plugins for TUIX framework",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    },
    "./system": {
      "import": "./dist/system/index.js",
      "require": "./dist/system/index.cjs",
      "types": "./dist/system/index.d.ts"
    }
  },
  "scripts": {
    "build": "bun run tsc && bun build src/index.ts --outdir dist --target node",
    "test": "bun test",
    "typecheck": "bun run tsc --noEmit"
  },
  "dependencies": {
    "@tuix/core": "workspace:*",
    "effect": "^3.10.0",
    "uuid": "^11.1.0",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/node": "^20.0.0",
    "@types/uuid": "^10.0.0",
    "typescript": "^5.0.0"
  },
  "peerDependencies": {
    "bun": "^1.0.0"
  }
}